<!DOCTYPE LA [

<!--   SGML DESCRIPTIONS                                     -->
<!--   +     Required and repeatable element                 -->
<!--   ?     Optional element                                -->
<!--   *     Optional and repeatable element                 -->
<!--   ,     elements must follow in this order              -->
<!--   |     "or" connector (pick one element)               -->
<!--   &     "and" connector (all must occur in any order)   -->

<!--       ELEMENT  MIN  CONTENT     -->
<!-- These are doc, docno, text, etc -->
<!--       ELEMENT     NAME     CONTENT  DEFAULT -->
<!ELEMENT  DOC              - -  (DOCNO & (DOCID* | DATE* | P* | 
BYLINE* | DATELINE* | SUBJECT* | TYPE* | SECTION* | LENGTH* | 
HEADLINE* | TEXT+ | TABLE* | ROWRULE* | CELLRULE* | TABLEROW* | 
TABLECELL* | CORRECTION-DATE* | CORRECTION* | GRAPHIC*)* )> 
<!ELEMENT  DOCNO            - -  (#PCDATA)+>   
<!ELEMENT  DOCID            - -  (#PCDATA)+>   
<!ELEMENT  DATE             - -  (P* | #PCDATA)*>   
<!ELEMENT  SECTION          - -  (P* | #PCDATA)*>   
<!ELEMENT  LENGTH           - -  (P* | #PCDATA)*>   
<!ELEMENT  HEADLINE         - -  (P* | TABLE* | #PCDATA)*>   
<!ELEMENT  BYLINE           - -  (P* | #PCDATA)*>   
<!ELEMENT  DATELINE         - -  (P* | #PCDATA)*>   
<!ELEMENT  TEXT             - -  (P* | TABLE* | ROWRULE* |
TABLEROW* | CELLRULE* | TABLECELL* | #PCDATA)*> 
<!ELEMENT  TYPE             - -  (P* | TABLE* | #PCDATA)*>   
<!ELEMENT  SUBJECT          - -  (P* | #PCDATA)*>   
<!ELEMENT  CORRECTION-DATE  - -  (P* | #PCDATA)*>   
<!ELEMENT  CORRECTION       - -  (P* | TABLE* | #PCDATA)*>   
<!ELEMENT  GRAPHIC          - -  (P* | #PCDATA)*>   
<!ELEMENT  ROWRULE          - -  (#PCDATA)*> 
<!ELEMENT  TABLEROW         - -  (CELLRULE* | TABLECELL* | #PCDATA)*>
<!ELEMENT  CELLRULE         - -  (#PCDATA)*>   
<!ELEMENT  TABLECELL        - -  (#PCDATA)*>
<!ELEMENT  P                - -  (#PCDATA)*>   
<!ELEMENT  TABLE            - -  (ROWRULE* | TABLEROW* | CELLRULE* | 
TABLECELL* | #PCDATA)*>   
<!ELEMENT LA                O O  (DOC)*>

<!ATTLIST     TABLE      CWL   CDATA     #IMPLIED 
