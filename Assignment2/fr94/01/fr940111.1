<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=14 g=1 f=1 -->
futures 
<!-- PJG 0012 frnewline -->
options on futures 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
Jurisdiction ``B'': 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=14 g=1 f=1 -->
futures 
<!-- PJG 0012 frnewline -->
options on futures 
<!-- PJG 0012 frnewline -->
options on commodities 
