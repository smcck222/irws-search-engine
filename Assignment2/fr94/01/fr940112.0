<DOC>
<DOCNO> FR940112-0-00073 </DOCNO>
<PARENT> FR940112-0-00020 </PARENT>
<TEXT>
 
<!-- PJG STAG 4700 -->

<!-- PJG ITAG l=84 g=1 f=4 -->
List of Subjects 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=83 g=1 f=4 -->
