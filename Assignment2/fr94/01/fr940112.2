<!-- PJG ITAG l=28 g=1 f=1 -->
MCC Rules, Art. V, R. 7, Section 4.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
4
