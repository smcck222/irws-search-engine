<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=21 g=1 f=1 -->
Dated: December 20, 1993. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</SUPPLEM>
