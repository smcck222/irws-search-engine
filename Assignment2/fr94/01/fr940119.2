<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Initial registration
<!-- PJG 007 0104 intable -->
$40
<!-- PJG 007 0104 intable -->
$45
<!-- PJG 0012 frnewline -->
Renewals
<!-- PJG 007 0104 intable -->
20
<!-- PJG 007 0104 intable -->
