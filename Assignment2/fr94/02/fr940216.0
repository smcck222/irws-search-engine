<TEXT>
 
<!-- PJG STAG 4700 -->

<!-- PJG ITAG l=83 g=1 f=1 -->
D. Range/Extrapolation 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
