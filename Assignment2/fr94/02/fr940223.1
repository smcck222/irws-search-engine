<!-- PJG ITAG l=55 g=1 f=1 -->
Extension of Compliance Date for Installation of Digital Flight Data Recorders on Stage 2 Airplanes
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
agency: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Federal Aviation Administration (FAA), DOT.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
