<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
(7) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Pelagic trawl
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
