<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
(6) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Non-trawl gear
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
