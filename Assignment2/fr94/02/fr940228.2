<!-- PJG 0012 frnewline -->

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->

<!-- PJG 0012 frnewline -->
4
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
15 U.S.C. 72s(b)(2) (1988).
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</FOOTNOTE>

<!-- PJG /ITAG -->

