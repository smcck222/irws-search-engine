<!-- PJG ITAG l=28 g=1 f=1 -->
9
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
This information is not available to specialists or traders on the floor.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
10
