PART 77_COMMUNICATIONS WITH REPRESENTED PERSONS 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=70 g=1 f=1 -->
Sec.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
77.1&blank;Purpose and authority. 
<!-- PJG 0012 frnewline -->
77.2&blank;Definitions. 
