<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Send written comments to William A. Wood, District Ranger, P.O. Box 190, Minturn, CO 81645.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</ADDRESS>
