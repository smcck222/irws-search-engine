<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 12 noon, Alaska local time (A.l.t.), March 21, 1994, until 12 noon, A.l.t., March 31, 1994.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</DATE>
