<!-- PJG ITAG l=10 g=1 f=2 -->
DATES: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
This rule is effective on April 7, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
