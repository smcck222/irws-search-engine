<!-- PJG ITAG l=55 g=1 f=1 -->

<DOCTITLE>Receipt of Application for Emergency Exemption to use 2,4&hyph;D; Solicitation of Public Comment </DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Environmental Protection Agency (EPA). 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
