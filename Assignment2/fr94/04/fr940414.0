<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 12 noon, Alaska local time (A.l.t.), 
<!-- PJG 0012 frnewline -->
May 28, 1994, through 12 noon, A.l.t., August 31, 1994. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</DATE>
<FURTHER>
<!-- PJG ITAG l=10 g=1 f=2 -->
