<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 OSM will accept written comments on the scope of the EIS until 5 p.m. Eastern Time on May 31, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
