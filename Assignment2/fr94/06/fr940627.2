<!-- PJG ITAG l=11 g=1 f=1 -->
acetone 
<!-- PJG 007 0104 intable -->
67&hyph;64&hyph;1
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
n
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
-amyl acetate, technical grade 
<!-- PJG 007 0104 intable -->
628&hyph;63&hyph;7
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
1-butanol 
