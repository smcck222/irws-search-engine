<!-- PJG ITAG l=10 g=1 f=2 -->
EFFECTIVE DATE: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
July 5, 1994.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</DATE>
