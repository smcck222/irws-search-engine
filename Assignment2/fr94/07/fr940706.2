<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
Time and date
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
: 1 p.m.&hyph;4 p.m., July 14, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
Place
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
