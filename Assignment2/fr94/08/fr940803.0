<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
(5) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Passenger automobile
<!-- PJG /ITAG -->

