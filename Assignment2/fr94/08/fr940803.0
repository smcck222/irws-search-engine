<!-- PJG ITAG l=10 g=1 f=2 -->
DATES: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
This rule is effective on August 3, 1994. Comments must be received on or before November 1, 1994.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</DATE>
