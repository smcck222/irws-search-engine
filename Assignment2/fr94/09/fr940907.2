(Catalog of Federal Domestic Assistance Program No. 93.778, 
<!-- PJG 0012 frnewline -->
Medical Assistance Program)
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=20 g=1 f=1 -->
(Catalog of Federal Domestic Assistance Program No. 93.773, 
<!-- PJG 0012 frnewline -->
Medicare_Hospital Insurance; and Program No. 93.774, 
<!-- PJG 0012 frnewline -->
Medicare_Supplementary Medical Insurance Program)
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

