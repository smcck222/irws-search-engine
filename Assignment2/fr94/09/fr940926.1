<!-- PJG ITAG l=55 g=1 f=1 -->
Export of American Ginseng Harvested in 1994&hyph;1996 Seasons 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
