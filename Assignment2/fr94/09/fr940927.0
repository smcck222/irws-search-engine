<TEXT>
 
<!-- PJG STAG 4700 -->

<TABLE>

<!-- PJG tableformat 5,L2,i1,6C,6C,6C,6C,6C -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

