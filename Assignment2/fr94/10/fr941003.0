<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 16 U.S.C. 1801 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
et seq.
<!-- PJG /ITAG -->

<!-- PJG QTAG 04 -->
<!-- PJG /QTAG -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=11 g=1 f=3 -->
2. In &sect;672.7, paragraph (k) is revised to read as follows: 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=3 -->
&sect;672.7 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=3 -->
