<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Notice; Record of Decision for the Patoka River National Wetlands Project.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=59 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

