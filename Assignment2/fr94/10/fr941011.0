<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
General asset accounts. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
(a) Scope. 
<!-- PJG 0012 frnewline -->
(b) Definitions. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=14 g=1 f=1 -->
