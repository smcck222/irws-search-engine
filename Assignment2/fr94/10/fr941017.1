<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=2 -->
7
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=2 -->
47 CFR 1.46.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</FOOTNOTE>

