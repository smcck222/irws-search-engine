<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=3 -->
supra, 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
note 8.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</FOOTNOTE>

<!-- PJG /ITAG -->

