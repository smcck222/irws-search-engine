<TEXT>
 
<!-- PJG STAG 4700 -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;76.964 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Written notification of changes in rates and services.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

