<ROWRULE>  </ROWRULE>
<TABLEROW>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="L" CVJ="C">
200 Medley Relay 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="L" CVJ="C">
