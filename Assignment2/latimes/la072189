<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="R" CVJ="C">
53 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="R" CVJ="C">
-6 
</TABLECELL>
