<ROWRULE>  </ROWRULE>
<TABLEROW>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="L" CVJ="C">
Dave Erhardt 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="L" CVJ="C">
Canoga Park 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="L" CVJ="C">
QB 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="R" CVJ="C">
6-2 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="R" CVJ="C">
