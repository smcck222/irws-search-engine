<!-- PJG ITAG l=21 g=1 f=1 -->

<!-- PJG 0012 frnewline -->
Issued at Golden, Colorado, December 20, 1993.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</SUPPLEM>
