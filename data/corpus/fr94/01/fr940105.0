<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
[Amended] 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
2. Section 20.41 Time limitations is amended by removing paragraphs (b)(5), (c), and (d). 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

