<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
 to exempt the acquisition by WESTCO PA of certain Conrail lines. 
<!-- PJG 0012 frnewline -->
</FOOTNOTE>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
