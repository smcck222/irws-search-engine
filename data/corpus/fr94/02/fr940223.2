<DOC>
<DOCNO> FR940223-2-00074 </DOCNO>
<PARENT> FR940223-2-00038 </PARENT>
<TEXT>
 
<!-- PJG STAG 4703 -->

<!-- PJG ITAG l=69 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>[File No. 932 3092]</USBUREAU>
<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->

<DOCTITLE>Jockey International, Inc.; Proposed Consent Agreement With Analysis To Aid Public Comment</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
