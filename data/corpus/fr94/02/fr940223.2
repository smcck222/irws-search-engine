<!-- PJG ITAG l=06 g=1 f=1 -->
Counsel,
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</SIGNER>
<SIGNJOB>
<!-- PJG ITAG l=04 g=1 f=1 -->
American Society of Interpreters.
<!-- PJG /ITAG -->
</SIGNJOB>
<!-- PJG /STAG -->
</TEXT>
