<!-- PJG ITAG l=11 g=1 f=3 -->
Cyanea shipmanii, Hesperocnide sandwicensis, Ischaemum 
<!-- PJG 0012 frnewline -->
byrone, Nothocestrum breviflorum 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
(as 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
N. 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
breviflorum var. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
breviflorum
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
), 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Portulaca sclerocarpa, 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
and 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Zanthoxylum hawaiiense 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
(as 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Z. hawaiiense 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
var. 
<!-- PJG /ITAG -->

