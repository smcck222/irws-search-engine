<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=3 -->
(Mirounga leonina)
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 for purposes of scientific research.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
