<!-- PJG /ITAG -->

<!-- PJG QTAG 04 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=11 g=1 f=1 -->
2. New &sect;173.21 is added to subpart A to read as follows:
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;173.21 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
