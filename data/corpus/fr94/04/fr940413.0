PART 945_DESIGNATED HOUSING_PUBLIC HOUSING DESIGNATED FOR OCCUPANCY BY DISABLED, ELDERLY, OR DISABLED AND ELDERLY
FAMILIES 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=72 g=1 f=1 -->
Subpart A_General 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

