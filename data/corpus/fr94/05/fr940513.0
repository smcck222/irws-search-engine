<!-- PJG ITAG l=28 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=5 f=1 -->
255
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
Panasonic, G&hyph;7, 2. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=5 f=1 -->
256
