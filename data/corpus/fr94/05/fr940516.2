<!-- PJG ITAG l=55 g=1 f=1 -->
1992 and 1993 Digital Audio Recording Royalty Funds; Motions and Objections 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
