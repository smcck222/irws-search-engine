<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 May 18, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</DATE>
