<DOCTITLE>Groundfish of the Bering Sea and Aleutian Islands </DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
