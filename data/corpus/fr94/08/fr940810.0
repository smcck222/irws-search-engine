<!-- PJG STAG 4700 -->

<!-- PJG ITAG l=21 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
Cleavage Fragments: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
