<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
) 
<!-- PJG 0012 frnewline -->
R=retardation, t=crystal thickness in &mu;m, and 
<!-- PJG 0012 frnewline -->
n
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=6 f=4 -->
