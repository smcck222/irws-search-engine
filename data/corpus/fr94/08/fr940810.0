<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=6 f=1 -->
m
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
m or longer, with a length-to-width 
<!-- PJG 0012 frnewline -->
ratio of 3 to 1 or longer. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
