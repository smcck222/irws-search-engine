<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=21 g=1 f=1 -->
Dated: August 4, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</SUPPLEM>
