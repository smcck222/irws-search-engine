<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=84 g=1 f=1 -->
1. El Paso Natural Gas Company 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

