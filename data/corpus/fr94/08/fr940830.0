<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Conversion to National banking association or State bank. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

