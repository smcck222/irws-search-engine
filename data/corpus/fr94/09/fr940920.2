<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
Businesses or other for-profit organizations. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
