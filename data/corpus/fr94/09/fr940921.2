<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Wisconsin Central Ltd._Purchase Exemption_Soo 
<!-- PJG 0012 frnewline -->
Line Railroad Company Line Between Superior and 
<!-- PJG 0012 frnewline -->
Ladysmith, WI 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
