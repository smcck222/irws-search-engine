<ACTION>
<!-- PJG ITAG l=10 g=1 f=2 -->
ACTION: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Interim notice establishing a 1994 aggregate production quota and request for comments.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=59 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
