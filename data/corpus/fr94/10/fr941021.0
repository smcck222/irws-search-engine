<!-- PJG ITAG l=28 g=1 f=1 -->
A list of commenters is attached as Appendix A.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
