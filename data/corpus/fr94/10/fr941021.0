<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 12 noon, Alaska local time (A.l.t.), October 17, 1994, until 12 midnight, A.l.t., December 31, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</DATE>
