<!-- PJG ITAG l=28 g=1 f=1 -->
6
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
Amex Rule 731 ``Resolution of Uncompared Transactions.''
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
