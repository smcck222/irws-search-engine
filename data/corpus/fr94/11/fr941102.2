<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=1 -->
Safeguards: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
