<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 11:30 p.m. local time November 4, 1994, through December 31, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</DATE>
<FURTHER>
<!-- PJG ITAG l=10 g=1 f=2 -->
