<!-- PJG ITAG l=28 g=1 f=1 -->
12
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
Section 8(a) of the Exchange Act. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

