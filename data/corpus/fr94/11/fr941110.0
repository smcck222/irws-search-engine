<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 December 12, 1994. 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

