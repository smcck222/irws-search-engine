<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=1 -->
 provisions
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Severe
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->
