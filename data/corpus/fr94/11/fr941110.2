<DOCTITLE>Freedom Support Act Secondary School Initiative_U.S./NIS Academic Studies Inbound/Outbound Program</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<ACTION>
<!-- PJG ITAG l=10 g=1 f=2 -->
ACTION: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Notice_Request for Proposals.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=59 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

