<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=11 g=1 f=1 -->
2. Section 482.21(b) is revised as follows: 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;482.21 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
