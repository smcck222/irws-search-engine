<!-- PJG ITAG l=11 g=1 f=1 -->
(a) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Definition. 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
(1) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Ex parte communication 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
