<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;747.9 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Ex parte communications. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

