<ROWRULE>  </ROWRULE>
<TABLEROW>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="L" CVJ="C">
Leland Lieberg, Alhambra 
</TABLECELL>
<CELLRULE>  </CELLRULE>
