<TABLEROW>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="R" CVJ="C">
9 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="R" CVJ="C">
8 
</TABLECELL>
<CELLRULE>  </CELLRULE>
<TABLECELL CHJ="R" CVJ="C">
